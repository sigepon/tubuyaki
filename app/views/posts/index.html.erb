<div class="main">
<% if logged_in? %>
    <%= render 'form' %>
<% end %>

<h1>Posts#index</h1>
<table>
    <% @posts.each do |post| %>
    <tr>
        <td class="body"><%= post.body %></td>
        <td class="destroy"><%= link_to 'destroy', post, method: :delete %></td>
    </tr>
    <% end %>
</table>
</div>
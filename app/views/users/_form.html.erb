<%= link_to '投稿一覧へ', posts_path %><br /><br />

<%= form_for @user, class: 'user_form' do |f| %>
  <label for="name">名前:</label> <%= f.text_field :name %><br />
  <label for="email">メールアドレス:</label> <%= f.text_field :email %><br />
  <label for="password">パスワード:</label> <%= f.password_field :password %><br />
  <label for="icon">アイコン画像:</label> <%= f.text_field :icon %><br />
  <div class="actions"><%= f.submit 'ユーザ登録' %></div>
<% end %>